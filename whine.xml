<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <TemplateURL>https://raw.githubusercontent.com/olig89/unraidtemplates/master/whine.xml</TemplateURL>
  <Beta>False</Beta>
  <Category>Other</Category>
  <Name>whine-and-cheese</Name>
  <Description>
    First you drink the wine, then you eat the cheese (DE)
  </Description>
  <Overview>
    First you drink the wine, then you eat the cheese (OV)
  </Overview>
  <Support>https://whine.gillflix.com</Support>
  <Registry>https://registry.hub.docker.com/u/loosescrew/whine_app_server/</Registry>
  <GitHub>https://github.com/loosescrew/whine_app_server</GitHub>
  <Repository>loosescrew/whine_app_server</Repository>
  <Project>https://whine.gillflix.com/</Project>
  <Deprecated>true</Deprecated>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>5000</HostPort>
        <ContainerPort>5000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/whine/config</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:5000]/</WebUI>
  <Banner></Banner>
  <Icon>https://raw.githubusercontent.com/loosescrew/whine_app_server/master/loosescrew/images/whine-icon.png</Icon>
  <ExtraParams></ExtraParams>
</Containers>
